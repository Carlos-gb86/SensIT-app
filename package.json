{
  "name": "sensit",
  "version": "1.0.0",
  "description": "SensIT Manager app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "devserver": "live-server",
    "start": "node server.js",
    "start:dev": "nodemon server.js -e js,json,pug,html,css",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch:js": "parcel ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Carlos Gil Berrocal",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "node-sass": "^4.5.3",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.1",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^4.1.1",
    "prettier": "^2.0.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.3",
    "chartjs": "^0.3.24",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "dropbox": "^4.0.30",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.1",
    "ftp": "^0.3.10",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb-extended-json": "^1.11.0",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "pug": "^2.0.4",
    "qs": "^6.9.3",
    "sharp": "^0.25.2",
    "slugify": "^1.4.0",
    "three": "^0.114.0",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    // "node": "^13.0.1",
    // "npm": "^6.12.0"
    "node": "13.x",
    "npm": "6.x"
    
  }
}
